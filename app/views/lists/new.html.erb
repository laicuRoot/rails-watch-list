<div class="container">
  <div class="row">
    <div class="col-sm-12 mt-3">
      <%= simple_form_for @list do |f| %>
        <%= f.input :name %>
        <%= f.input :picture, as: :file %>
        <%= f.submit 'Create List', class:'btn btn-success btn-sm' %>
      <% end %>
    </div>
  </div>
</div>
