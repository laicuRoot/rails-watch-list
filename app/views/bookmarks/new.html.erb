<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <%= simple_form_for [@list, @bookmark] do |f| %>
        <%= f.association :movie %>
        <%= f.input :comment %>
        <%= f.button :submit, class: 'btn btn-outline-success' %>
      <% end %>
    </div>
  </div>
</div>
